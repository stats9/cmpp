# Dynamically load the shared library and register C++ functions
useDynLib(cmpp, .registration = TRUE)

# Export C++ functions that you want to make available in R
export(Initialize)
export(cdf_gomp)
export(pdf_gomp) 
export(GetDim)
export(LogLike1)
export(compute_grad)
export(makeMat)
export(Cleanup)
export(compute_hessian)
export(estimate_parameters)
export(bootstrap_variance)
export(CIF_res1)
export(CIF_Figs)
export(f_pdf_rcpp)
export(F_cdf_rcpp)
export(log_f_rcpp)
export(compute_log_f_gradient_rcpp)
export(compute_log_f_hessian_rcpp)
export(estimate_parameters2)